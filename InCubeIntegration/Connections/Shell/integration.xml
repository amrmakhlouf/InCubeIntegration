	<UPSLT>
		<Query>SELECT convert (date,TSDBA.RS_SESSION.SESSION_DATE,102) AS ROUTE_DATE, TSDBA.RS_SESSION.REGION_ID, TSDBA.RS_SESSION.DESCRIPTION, TSDBA.RS_ROUTE.DRIVER1_ID, TSDBA.TS_EMPLOYEE.FIRST_NAME + ' ' + TSDBA.TS_EMPLOYEE.LAST_NAME AS Driver_Name, TSDBA.RS_ROUTE_EQUIPMENT.EQUIPMENT_ID, TSDBA.RS_STOP.LOCATION_ID, REPLACE(TSDBA.TS_LOCATION.DESCRIPTION, '''', '''''') AS Customer_Name, REPLACE(TSDBA.TS_LOCATION.ADDR_LINE1, '''', '''''') AS ADDR_LINE1, TSDBA.RS_ORDER.ORDER_NUMBER, TSDBA.RS_LINE_ITEM.SKU, TSDBA.RS_LINE_ITEM.SIZE1, TSDBA.RS_LINE_ITEM.SIZE2, TSDBA.RS_LINE_ITEM.SIZE3, STUFF(TSDBA.TS_LOCATION.LATITUDE, 3, 0, '.') AS LATITUDE, STUFF(TSDBA.TS_LOCATION.LONGITUDE, 3, 0, '.') AS LONGITUDE, TSDBA.RS_ROUTE.ROUTE_ID, TSDBA.RS_STOP.SEQUENCE_NUMBER, LTRIM(RIGHT(CONVERT(VARCHAR(20), DATEADD(hour, 4, TSDBA.RS_STOP.ARRIVAL), 100), 7)) AS ARRIVAL, TSDBA.RS_STOP.SERVICE_TIME / 60 AS service_time, TSDBA.RS_STOP.TRAVEL_TIME / 60 AS Travel_Time, TSDBA.RS_STOP.DISTANCE / 100.0 AS Distance , TSDBA.RS_STOP.TRIP_NUMBER FROM TSDBA.RS_SESSION INNER JOIN TSDBA.RS_ROUTE ON TSDBA.RS_SESSION.PKEY = TSDBA.RS_ROUTE.RN_SESSION_PKEY INNER JOIN TSDBA.RS_ROUTE_EQUIPMENT ON TSDBA.RS_ROUTE_EQUIPMENT.ROUTE_PKEY = TSDBA.RS_ROUTE.PKEY INNER JOIN TSDBA.RS_STOP ON TSDBA.RS_STOP.ROUTE_PKEY = TSDBA.RS_ROUTE.PKEY INNER JOIN TSDBA.RS_ORDER ON TSDBA.RS_ORDER.STOP_PKEY = TSDBA.RS_STOP.PKEY LEFT OUTER JOIN TSDBA.RS_LINE_ITEM ON TSDBA.RS_LINE_ITEM.RN_ORDER_PKEY = TSDBA.RS_ORDER.PKEY LEFT JOIN TSDBA.TS_EMPLOYEE ON TSDBA.TS_EMPLOYEE.ID = TSDBA.RS_ROUTE.DRIVER1_ID AND TSDBA.RS_SESSION.REGION_ID = TSDBA.TS_EMPLOYEE.REGION_ID INNER JOIN TSDBA.TS_LOCATION ON TSDBA.TS_LOCATION.ID = TSDBA.RS_STOP.LOCATION_ID AND TSDBA.TS_LOCATION.REGION_ID = TSDBA.RS_SESSION.REGION_ID and TSDBA.TS_LOCATION.TYPE = 'SIT' WHERE (UPPER(TSDBA.RS_ROUTE.STATUS) = 'BUILT') AND TSDBA.RS_ROUTE.TERRITORY_PKEY = 0 AND convert(date,TSDBA.RS_SESSION.SESSION_DATE,102) >= convert(date, GETDATE(),102)</Query>
